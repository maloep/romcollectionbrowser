      <Scraper parseInstruction="02 - thegamesdb.xml" source="http://thegamesdb.net/api/GetGame.php?name=%GAME%&amp;platform=%PLATFORM%" />
    </Site>
    <Site descFilePerGame="True" name="giantbomb.com" searchGameByCRC="False">
